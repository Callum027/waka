{
  "name": "dymajo-transit",
  "version": "1.0.0",
  "description": "Web App for Auckland Transport Real Time Board",
  "main": "index.js",
  "scripts": {
    "build-js": "browserify js/app.tsx -p [ tsify ] -o dist/app.js",
    "watch-js": "watchify js/app.tsx -p [ tsify ] -o dist/app.js",
    "build-css": "node-sass scss -o dist",
    "watch-css": "node-sass scss -o dist && node-sass -w scss -o dist",
    "install-typings": "typings install",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/consindo/dymajo-transit.git"
  },
  "keywords": [
    "auckland",
    "transport",
    "at",
    "hop",
    "transit",
    "dyamjo"
  ],
  "author": "Jono Cooper <jono@jono.nz>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/consindo/dymajo-transit/issues"
  },
  "homepage": "https://transit.dymajo.com",
  "devDependencies": {
    "browserify": "^13.1.0",
    "concurrently": "^2.2.0",
    "node-sass": "^3.8.0",
    "tsify": "^1.0.3",
    "typescript": "^1.8.10",
    "typings": "^1.3.2",
    "watchify": "^3.7.0"
  },
  "dependencies": {
    "autotrack": "^1.0.3",
    "azure-storage": "^1.2.0",
    "body-parser": "^1.15.2",
    "clipboard": "^1.5.12",
    "compression": "^1.6.2",
    "express": "^4.14.0",
    "imagemin": "^5.2.2",
    "imagemin-webp": "^4.0.0",
    "leaflet": "^1.0.0-rc.3",
    "moment": "^2.14.1",
    "moment-timezone": "^0.5.5",
    "react": "^15.3.0",
    "react-dom": "^15.3.0",
    "react-leaflet": "^1.0.0-beta.3",
    "react-router": "^2.6.1",
    "react-tap-event-plugin": "^1.0.0",
    "request": "^2.74.0",
    "reqwest": "^2.0.5",
    "sendgrid": "^4.3.0"
  }
}
