{
  "name": "dymajo-transit",
  "version": "1.0.0",
  "description": "Auckland Transport Progressive Web App",
  "main": "index.js",
  "scripts": {
    "build-js": "cross-env NODE_ENV=production webpack --progress --colors -p --display-optimization-bailout",
    "build-js-dev": "webpack --progress --colors",
    "watch-js": "webpack --progress --colors --watch -d",
    "build-css": "node-sass scss -o dist --output-style compressed",
    "build-css-dev": "node-sass scss -o dist --source-map dist",
    "watch-css": "node-sass scss -o dist && node-sass -w scss -o dist --source-map dist",
    "install-typings": "typings install",
    "anal": "webpack --json > report.json",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/consindo/dymajo-transit.git"
  },
  "keywords": [
    "auckland",
    "transport",
    "at",
    "hop",
    "transit",
    "dymajo"
  ],
  "author": "Jono Cooper <jono@jono.nz>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/consindo/dymajo-transit/issues"
  },
  "homepage": "https://transit.dymajo.com",
  "devDependencies": {
    "babel-core": "^6.25.0",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.1.1",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-preset-env": "^1.6.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.16.0",
    "cross-env": "^5.0.4",
    "eslint": "^4.4.0",
    "eslint-plugin-react": "^7.1.0",
    "node-sass": "^4.5.3",
    "webpack": "^3.4.1",
    "webpack-bundle-analyzer": "^2.9.0",
    "whatwg-fetch": "^2.0.1"
  },
  "dependencies": {
    "autotrack": "^2.4.1",
    "azure-storage": "^2.2.2",
    "body-parser": "^1.17.2",
    "clipboard": "^1.7.1",
    "csv-parse": "^1.2.1",
    "express": "^4.15.4",
    "extract-zip": "^1.6.0",
    "imagemin-webp": "^4.0.0",
    "leaflet": "^1.1.0",
    "moment": "^2.14.1",
    "moment-timezone": "^0.5.5",
    "mssql": "^4.0.4",
    "pug": "^2.0.0-rc.3",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-leaflet": "^1.4.1",
    "react-router": "^4.1.2",
    "react-router-dom": "^4.1.2",
    "react-tap-event-plugin": "^2.0.1",
    "react-transition-group": "^2.2.0",
    "request": "^2.74.0",
    "sendgrid": "^4.3.0",
    "stream-transform": "^0.1.2",
    "wkx": "^0.4.0",
    "zenscroll": "^4.0.0"
  }
}
