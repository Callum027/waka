{
  "name": "dymajo-transit",
  "version": "1.0.0",
  "description": "Auckland Transport Progressive Web App",
  "main": "index.js",
  "scripts": {
    "build-js": "cross-env NODE_ENV=production webpack -p",
    "build-js-dev": "webpack",
    "watch-js": "webpack --progress --colors --watch -d",
    "build-css": "node-sass scss -o dist --output-style compressed",
    "build-css-dev": "node-sass scss -o dist --source-map dist",
    "watch-css": "node-sass scss -o dist && node-sass -w scss -o dist --source-map dist",
    "install-typings": "typings install",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/consindo/dymajo-transit.git"
  },
  "keywords": [
    "auckland",
    "transport",
    "at",
    "hop",
    "transit",
    "dymajo"
  ],
  "author": "Jono Cooper <jono@jono.nz>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/consindo/dymajo-transit/issues"
  },
  "homepage": "https://transit.dymajo.com",
  "devDependencies": {
    "babel-core": "^6.20.0",
    "babel-loader": "^6.2.9",
    "babel-preset-latest": "^6.16.0",
    "babel-preset-react": "^6.16.0",
    "cross-env": "^3.1.3",
    "eslint-plugin-react": "^6.8.0",
    "node-sass": "^3.8.0",
    "typings": "^1.3.2",
    "webpack": "^1.13.3"
  },
  "dependencies": {
    "autotrack": "^1.0.3",
    "azure-storage": "^1.2.0",
    "body-parser": "^1.15.2",
    "clipboard": "^1.5.12",
    "compression": "^1.6.2",
    "express": "^4.14.0",
    "imagemin": "^5.2.2",
    "imagemin-webp": "^4.0.0",
    "leaflet": "^1.0.0-rc.3",
    "moment": "^2.14.1",
    "moment-timezone": "^0.5.5",
    "react": "^15.4.0",
    "react-dom": "^15.4.0",
    "react-leaflet": "^1.0.0-beta.3",
    "react-router": "^2.6.1",
    "react-tap-event-plugin": "^2.0.1",
    "request": "^2.74.0",
    "reqwest": "^2.0.5",
    "sendgrid": "^4.3.0",
    "wkx": "^0.4.0"
  }
}
